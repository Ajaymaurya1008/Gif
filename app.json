{
  "expo": {
    "name": "Gif",
    "slug": "Gif",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/GIF.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/GIF.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ajaymaurya1008.Gif"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/GIF.png"
    },
    "plugins": [
      "expo-router",
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "237bb50f-a78a-4845-933c-ab2180cf6caf"
      }
    }
  }
}
